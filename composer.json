{
  "name": "avtonom/sms-streamtelecom-bundle",
  "type": "symfony-bundle",
  "description": "Provider to send SMS notifications for Symfony2 bundle for PHP",
  "keywords": ["Symfony2", "bundle", "sms", "streamtelecom", "szk-info", "stream-telecom", "szk", "notification"],
  "homepage": "https://github.com/Avtonom/sms-streamtelecom-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton U",
      "email": "avtonomspb@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/dependency-injection": "~2.3|3.*",
    "symfony/monolog-bundle": "2.*",
    "kphoen/sms-sender-bundle": "dev-master"
  },
  "autoload": {
    "psr-0": { "Avtonom\\SmsStreamtelecomBundle": "" }
  },
  "target-dir": "Avtonom/SmsStreamtelecomBundle",
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}